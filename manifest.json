{
    "manifest_version": 3,
    "name": "Image format Converter",
    "version": "1.0",
    "description": "convert your images from one format to another with a click of a button",
    "icons": {
        "128": "128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "128.png",
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "downloads", "contextMenus"]
}